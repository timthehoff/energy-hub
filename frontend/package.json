{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV='production' node dist/server.js",
    "watch-server": "concurrently -r \"tsc -w -p tsconfig-server.json\" \"nodemon -r dotenv/config dist/server.js\"",
    "dev": "vite",
    "build": "tsc && vite build && tsc -p tsconfig-server.json",
    "preview": "vite preview",
    "watch": "concurrently \"npm run watch-server\" \"npm run dev\"",
    "lint": "prettier -w --single-quote 'server/**/*' 'src/**/*' '*.(html|ts|json|js)'"
  },
  "devDependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fontsource/roboto": "^4.5.7",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.1",
    "@types/cookie": "^0.5.1",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/set-cookie-parser": "^2.4.2",
    "@types/url-safe-base64": "^1.1.0",
    "@vitejs/plugin-react": "^2.0.0",
    "concurrently": "^7.3.0",
    "crypto-random-string": "^5.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "typescript": "^4.6.4",
    "url-safe-base64": "^1.2.0",
    "vite": "^3.0.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "cookie": "^0.5.0",
    "debug-http": "^1.1.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "node-fetch": "^3.2.9",
    "set-cookie-parser": "^2.5.1"
  }
}
